{
    "strategy": {
        "mode": "loadbalance"
    },
    "targets": [
        {
            "provider": "openai",
            "virtual_key": "openai-virtual-key"
        },
        {
            "strategy": {
                "mode": "fallback",
                "on_status_codes": [
                    429,
                    241
                ]
            },
            "targets": [
                {
                    "virtual_key": "azure-virtual-key"
                },
                {
                    "virtual_key": "openai-virtual-key"
                }
            ]
        }
    ]
}